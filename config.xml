<?xml version="1.0" encoding="UTF-8"?>
<widget xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0" id="gcians1234567"  version = "1.0.0" versionCode = "19" >


        <name>GC OSWS CONNECT</name>
        <description>GC OSWS Mobile Application</description>
        <author>doubleQuote</author>        
		<gap:platforms>
                <gap:platform name="ios" />
                <gap:platform name="android" />
                <gap:platform name="webos" />
                <gap:platform name="symbian.wrt" />
                <gap:platform name="blackberry" project="widgets"/>
        </gap:platforms>

        <icon src="logo.png" gap:role="default" />
